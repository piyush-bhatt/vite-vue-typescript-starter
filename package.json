    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.2",
